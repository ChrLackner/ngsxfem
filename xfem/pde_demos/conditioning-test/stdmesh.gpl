set terminal pdfcairo dashed
set output "stdmesh.cond.pdf"
set logscale y
set y2tics
offset=0.3
plot "out.stdmesh.cond.A" u ($0/1000+offset):3 w l lw 2 title "cond(A)", \
     "out.stdmesh.cond.CinvA" u ($0/1000+offset):3 w l lw 2 title "cond(CinvA)", \
     "out.stdmesh.its" u ($0/1000+offset):2 w l axes x1y2 title "CG its. "
     
